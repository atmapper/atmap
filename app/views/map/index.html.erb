function markersearch(markersrc, marker, json) {
	var div = document.createElement("div");
		var aaa = document.createElement("img");
		aaa.src = markersrc;
		aaa.style.verticalAlign = "top";
		aaa.style.height = "40px";
		aaa.style.width = "40px";
		var b = document.createElement("b");
		b.innerHTML = json['mapname'];
		var br = document.createElement("br");
		var span = document.createElement("span");
		span.innerHTML = json['memo'];
		var aaaaa = document.createElement("a");
		aaaaa.href = 'javascript:void(0);';
		aaaaa.onclick = function() {
			route_set(json['lat'],json['lgn']);
		}
		aaaaa.innerHTML = 'ルート案内（試験機能）';
	div.appendChild(aaa);
	div.appendChild(b);
	div.appendChild(span);
	div.appendChild(aaaaa);
	document.getElementById('atmlist').appendChild(div);
